{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "Node",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
    "removeComments": true,
    "sourceMap": false,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "downlevelIteration": true,
    "outDir": ".temp"
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "node_modules/**/*.ts"
  ]
}