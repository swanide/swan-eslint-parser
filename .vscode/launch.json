{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Test",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/.debug/test/debug.js",
            "preLaunchTask": "tsc: build - tsconfig-debug.json",
            "outFiles": [
                "${workspaceFolder}/.debug/**/*.js"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch lint-swan",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "runtimeArgs": [
                "-r",
                "ts-node/register"
            ],
            "args": [
                "${workspaceFolder}/test/example/lint-swan.js"
            ],
            "protocol": "inspector"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch parse-swan",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "runtimeArgs": [
                "-r",
                "ts-node/register"
            ],
            "args": [
                "${workspaceFolder}/test/example/parse-swan.js"
            ],
            "protocol": "inspector"
        }
    ]
}