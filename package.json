{
  "name": "swan-xml-parser",
  "version": "0.0.1",
  "description": "swan xml parser",
  "main": "index.js",
  "files": [
    "index.*"
  ],
  "scripts": {
    "prebuild": "npm run -s clean",
    "build:es": "tsc --module es2015",
    "build": "tsc --module es2015 && rollup -c -o index.js && dts-bundle --name mpxml-eslint-parser --main .temp/index.d.ts --out ../index.d.ts",
    "clean": "rm -rf .nyc_output .temp coverage index.*",
    "test": "npm run -s test:mocha",
    "test:mocha": "nyc mocha \"test/spec/*.js\" --reporter dot --timeout 10000"
  },
  "author": "kekee000@gmail.com",
  "license": "MIT",
  "dependencies": {
    "@types/estree": "0.0.50",
    "assert": "^2.0.0",
    "debug": "^4.3.2",
    "eslint-scope": "^6.0.0",
    "eslint-visitor-keys": "^1.3.0",
    "espree": "^6.2.0",
    "esquery": "^1.4.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/assert": "^1.5.6",
    "@types/debug": "^4.1.7",
    "@types/eslint-scope": "^3.7.1",
    "@types/eslint-visitor-keys": "^1.0.0",
    "@types/lodash": "^4.14.176",
    "@types/node": "^16.11.6",
    "dts-bundle": "^0.7.3",
    "eslint": "^7.32.0",
    "rollup": "^2.60.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.3"
  }
}
